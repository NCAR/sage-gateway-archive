<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

    <bean id="datasetDocumentType" class="java.lang.String">
        <constructor-arg index="0" value="Dataset"/>
    </bean>
    <bean id="projectDocumentType" class="java.lang.String">
        <constructor-arg index="0" value="Project"/>
    </bean>
    <bean id="softwareDocumentType" class="java.lang.String">
        <constructor-arg index="0" value="Software"/>
    </bean>
    <bean id="remoteIndexableDocumentType" class="java.lang.String">
        <constructor-arg index="0" value="RemoteIndexable"/>
    </bean>

</beans>
